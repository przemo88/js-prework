{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-param-reassign":["error", { "props": false }],
        "class-methods-use-this": "off",
        "func-names": ["error", "never"],
        "no-unused-vars": "warn",
        "no-useless-escape": "off"
    }
}